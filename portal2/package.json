{
  "name": "portal2",
  "version": "0.0.1",
  "private": true,
  "description": "new Portal project base on vue-admin-template",
  "author": "Jie.",
  "scripts": {
    "test:unit": "jest --clearCache && vue-cli-service test:unit",
    "lint": "eslint --ext .js,.vue src",
    "build:prod": "vue-cli-service build",
