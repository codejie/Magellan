{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "typescript",
			"tsconfig": "tsconfig.json",
			"problemMatcher": [
				"$tsc"
			],
			"group": "build",
			"label": "tsc: build - tsconfig.json",
			"dependsOn":[
				"copy graphql"
			]
		},
		{
			"type": "shell",
			"label": "copy graphql",
			"command": "cp",
			"args":[
				"-Force",
				"-r",
				"./src/gl/schema",
				"./dist/src/gl/schema"
			]
		}
	]
}